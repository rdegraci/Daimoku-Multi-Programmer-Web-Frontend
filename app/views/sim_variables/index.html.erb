<h1>Listing sim_variables</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
  </tr>

<% @sim_variables.each do |sim_variable| %>
  <tr>
    <td><%=h sim_variable.name %></td>
    <td><%=h sim_variable.value %></td>
    <td><%= link_to 'Show', user_sim_variable_path(@user, sim_variable) %></td>
    <td><%= link_to 'Edit', edit_user_sim_variable_path(@user, sim_variable) %></td>
    <td><%= link_to 'Destroy', user_sim_variable_path(@user, sim_variable), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New sim_variable', new_user_sim_variable_path(@user) %> |
<%= link_to 'User Homepage', user_path(@user) %>